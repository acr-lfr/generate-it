{
  "name": "generate-it",
  "version": "5.23.2",
  "description": "Generate it, socket and html consumers and clients from yml files (openapi/asyncapi)",
  "author": "Acrontum GmbH",
  "license": "MIT",
  "main": "./build/openapiNodegen.js",
  "bin": {
    "generate-it": "./build/cli.js"
  },
  "scripts": {
    "install:githooks": "cp ./githooks/* .git/hooks",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "npm run build && jest --runInBand",
    "test:unit:single": "jest --collect-coverage false",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "build:tocs": "npx doctoc .",
    "build": "rm -fR ./build && ttsc -p .",
    "build:watch": "ttsc --watch -p .",
    "start": "node cli.js",
    "prepublishOnly": "npm run test"
  },
  "homepage": "https://acrontum.github.io/generate-it",
  "repository": {
    "type": "git",
    "url": "https://github.com/acrontum/generate-it"
  },
  "keywords": [
    "asyncapi",
    "swagger",
    "openapi",
    "node",
    "codegen",
    "generator",
    "generate-it"
  ],
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^5.1.0",
    "compare-versions": "^3.6.0",
    "deepmerge": "^4.2.2",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "js-yaml": "^3.14.0",
    "json-schema-ref-parser": "^9.0.6",
    "json-schema-resolve-allof": "^1.5.0",
    "lodash": "^4.17.19",
    "md5-file": "^5.0.0",
    "npm-tool-version-check": "^1.1.2",
    "nunjucks": "^3.2.2",
    "prettier": "^2.0.5",
    "project-name-generator": "^2.1.7",
    "quicktype": "^15.0.256",
    "recursive-copy": "^2.0.10",
    "semver": "^7.3.2",
    "text-file-diff": "^1.1.2",
    "uglify-es": "^3.3.9",
    "walk": "^2.3.14",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.1",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.3",
    "@types/js-yaml": "^3.12.5",
    "@types/json-schema": "^7.0.5",
    "@types/lodash": "^4.14.159",
    "@types/node": "^13.13.15",
    "@types/nunjucks": "^3.1.3",
    "@types/prettier": "^2.0.2",
    "@types/semver": "^7.3.1",
    "@types/walk": "^2.3.0",
    "@types/yamljs": "^0.2.31",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "codecov": "^3.7.2",
    "eslint": "^6.8.0",
    "fdir": "^4.1.0",
    "folder-hash": "^3.3.2",
    "hasha": "^5.2.0",
    "jest": "^25.5.4",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.3",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.7"
  }
}
